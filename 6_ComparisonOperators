#include <iostream>
#include <array>

using namespace std;

int main(){
    array<int, 4> arr = { 1, 2, 3, 4 };
    array<int, 4> arr2 = { 5, 6, 7, 8 };
    
    cout << "arr" << endl;

    for (auto el: arr) {
        cout << el << " ";
    }

    cout << endl;

    cout << "arr2" << endl;

    for (auto el : arr2) {
        cout << el << " ";
    }
    
    cout << endl;

    cout << "Все элементы равны: arr == arr2: " << (arr == arr2) << endl;
    cout << "Хотя бы один элемент больше: arr > arr2: " << (arr > arr2) << endl;
    cout << "Хотя бы один элемент меньше: arr < arr2: " << (arr < arr2) << endl;
    cout << "Хотя бы один элемент больше или равно: arr >= arr2: " << (arr >= arr2) << endl;
    cout << "Хотя бы один элемент меньше или равно: arr <= arr2: " << (arr <= arr2) << endl;

    cout << endl;
    cout << "arr = { 1, 7, 3, 4 }" << endl;
    arr = { 1, 7, 3, 4 };
    for (auto el : arr) {
        cout << el << " ";
    }

    cout << endl;

    cout << "arr2" << endl;

    for (auto el : arr2) {
        cout << el << " ";
    }

    cout << endl;

    cout << "Все элементы равны: arr == arr2: " << (arr == arr2) << endl;
    cout << "Хотя бы один элемент больше: arr > arr2: " << (arr > arr2) << endl;
    cout << "Хотя бы один элемент меньше: arr < arr2: " << (arr < arr2) << endl;
    cout << "Хотя бы один элемент больше или равно: arr >= arr2: " << (arr >= arr2) << endl;
    cout << "Хотя бы один элемент меньше или равно: arr <= arr2: " << (arr <= arr2) << endl;

    return 0;
}
